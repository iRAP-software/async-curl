{
    "name": "programster/curl-easier",
    "type": "library",
    "description": "A package to make it easier to send curl request asynchronously using [curl_multi_exec](https://secure.php.net/manual/en/function.curl-multi-exec.php).",
    "keywords": ["curl"],
    "homepage": "https://github.com/programster/curl-easier",
    "license": "MIT",
    "authors": [
        {
            "name": "Stuart Page",
            "email": "stuart.page@irap.org",
            "homepage": "http://irap.org/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.5.0"
    },
    "autoload": {
        "psr-4": {
            "Programster\\CurlEasier\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Programster\\CurlEasier\\Testing\\": "testing",
            "Programster\\CurlEasier\\Testing\\Tests\\": "testing/tests"
        }
    }
}